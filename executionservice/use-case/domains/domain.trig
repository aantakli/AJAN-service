mosim:Mapping {

	:WorkerMapping
		rml:logicalSource [
			rml:source [
				a carml:Stream ;
			] ;
			rml:referenceFormulation ql:JSONPath;
			rml:iterator "$.workers[*]"
		] ;

		rr:subjectMap [
			rr:template "http://www.ajan.de/ajan-mapping-ns#Worker/{workerid}" ;
