WHERE {
	 ?avatar mosim:hand "BothHands" .
}"""^^xsd:string. _:b337 <http://www.w3.org/1999/02/22-rdf-syntax-ns#first> <http://localhost:8090/rdf4j/repositories/behaviors#Action-fd196a5f-32b6-4ae3-82b7-896bb252f420>. _:b337 <http://www.w3.org/1999/02/22-rdf-syntax-ns#rest> _:b338. <http://localhost:8090/rdf4j/repositories/behaviors#Action-fd196a5f-32b6-4ae3-82b7-896bb252f420> <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.ajan.de/behavior/bt-ns#Action>. <http://localhost:8090/rdf4j/repositories/behaviors#Action-fd196a5f-32b6-4ae3-82b7-896bb252f420> <http://www.ajan.de/behavior/bt-ns#definition> <http://localhost:8090/rdf4j/repositories/services#GenericAsyncInstruction>. <http://localhost:8090/rdf4j/repositories/behaviors#Action-fd196a5f-32b6-4ae3-82b7-896bb252f420> <http://www.ajan.de/behavior/bt-ns#inputs> _:b285. <http://localhost:8090/rdf4j/repositories/behaviors#Action-fd196a5f-32b6-4ae3-82b7-896bb252f420> <http://www.w3.org/2000/01/rdf-schema#comment> """"""^^xsd:string. <http://localhost:8090/rdf4j/repositories/behaviors#Action-fd196a5f-32b6-4ae3-82b7-896bb252f420> <http://www.w3.org/2000/01/rdf-schema#label> """reachLeft(Object)"""^^xsd:string. _:b285 <http://www.w3.org/1999/02/22-rdf-syntax-ns#first> _:b222. _:b285 <http://www.w3.org/1999/02/22-rdf-syntax-ns#rest> <http://www.w3.org/1999/02/22-rdf-syntax-ns#nil>. _:b222 <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.ajan.de/behavior/bt-ns#ConstructQuery>. _:b222 <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.ajan.de/behavior/bt-ns#Input>. _:b222 <http://www.ajan.de/behavior/bt-ns#originBase> <http://www.ajan.de/ajan-ns#AgentKnowledge>. _:b222 <http://www.ajan.de/behavior/bt-ns#sparql> """PREFIX mosim: <http://www.dfki.de/mosim-ns#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX test: <http://test/>

CONSTRUCT {
	test:Instruction mosim:mmu "Pose/Reach" .
	test:Instruction mosim:actionName "ReachL" .
	test:Instruction mosim:mmuProperty test:TargetProperty .
	test:Instruction mosim:mmuProperty test:HandProperty .
	
	test:TargetProperty mosim:key "TargetID" .
	test:TargetProperty mosim:value ?id .
	
	test:HandProperty mosim:key "Hand" .
	test:HandProperty mosim:value "Left" .

	?cosim rdf:type mosim:CoSimulator .
	?cosim mosim:host ?host .
	?cosim mosim:port ?port .
}
WHERE {
	?avatar rdf:type mosim:Avatar .
	test:PositionGoalPart rdf:value ?part .
	?part mosim:type ?partName .
	
	?target rdf:type mosim:MSceneObject .
	?target rdfs:label ?label .
	?target mosim:id ?id .
	
	FILTER (CONTAINS(?label, STR(?partName))).
	FILTER (CONTAINS(?label, "GraspL")).
	
	?cosim rdf:type mosim:CoSimulator .
